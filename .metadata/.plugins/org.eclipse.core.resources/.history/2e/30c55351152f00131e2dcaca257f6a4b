var moveCount=0;
var turn = 0;

function AI(){
	if(label1.text==' ' && turn == 1){
		label1.text='O';
		turn=0;
	}
	if(label2.text==' ' && turn == 1){
		label1.text='O';
		turn=0;
	}
	if(label3.text==' ' && turn == 1){
		label1.text='O';
		turn=0;
	}
	if(label4.text==' ' && turn == 1){
		label1.text='O';
