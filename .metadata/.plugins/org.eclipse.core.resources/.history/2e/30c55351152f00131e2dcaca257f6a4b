		turn=0;
	}
	if(label7.text==' ' && turn == 1){
		label1.text='O';
		turn=0;
	}
	if(label8.text==' ' && turn == 1){
		label1.text='O';
		turn=0;
	}
	if(label9.text==' ' && turn == 1){
		label1.text='O';
		turn=0;
	}
}



