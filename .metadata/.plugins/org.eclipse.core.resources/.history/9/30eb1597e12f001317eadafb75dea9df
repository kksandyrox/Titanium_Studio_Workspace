var window = Ti.UI.createWindow({
	backgroundcolor:'21DCEE'
});

var tabelView = Ti.UI.createTableView();
var tableData = [];

var file = Ti.Filesystem.getFile(Ti.Filesystem.resourcesDirectory, "wordsAndMeanings.txt"); 

for(var i=0;i<words.length ; i++){
	word = words[i];
	row = Ti.UI.createTableViewRow({
		height : '60dp'
	});
	nameLabel = Ti.UI.createLabel({
		text: word.name,
		font: { fontSize:'24dp', fontWeight:'bold'},
		height:'auto',
		left:'10dp',
		color:'#000',
		touchEnabled : false
	});
	row.add(nameLabel);
	tableData.push(row);
	tableView.setData(tableData);
}

window.open();
window.add(tableView);


