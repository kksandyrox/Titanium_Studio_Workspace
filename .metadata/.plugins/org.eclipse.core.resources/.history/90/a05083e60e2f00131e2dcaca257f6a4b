var window = Ti.UI.createWindow({
	backgroundColor:'white'
});

var view1 = Ti.UI.createView({
	backgroundColor:'yellow'
});

var label1 = Ti.UI.createLabel({
	backgroundColor: 'red',
	color:'black',
    text: ' ',
  
  top: 30,
  width: 40, 
  height: 40
});

window.open();
window.add(view1);
view1.add(label1);

